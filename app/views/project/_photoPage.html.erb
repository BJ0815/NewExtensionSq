<div id="flickr">
  <!-- Main -->
    <div id="main" class="wrapper">
      <div class="container">
        <header class="major">
          <h2><%= title%></h2>
          <p></p>
        </header>
      </div>
    </div>
  <!-- Main -->
    <div id="masonry" class="container-fluid">
      <div class="row no-gutters">
      <% albums.photo.each do |p| %>
        <div class="item col-xs-12 col-md-5 col-lg-3 img-wrapper">

          <%= image_tag('loading.svg',data: {"original"=>image_path(p.url_m)}, :title => p.title, :border => 0) %>
          <div class="img-overlay">
            <i class="fa fa-plus-circle" aria-hidden="true"></i>
          </div>
        </div>
      <% end %>
      </div>
    </div>
</div>
