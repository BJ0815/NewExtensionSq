<div class="flickr">
  <!-- Main -->
    <ul class="masonry" id="proc">
    <% results.each do |p| %>
      <li class="item">
        <h3><%= p.title %></h3>
        <%= link_to(image_tag('loading.svg',data: {"original"=>image_path(p.primary_photo_extras.url_m)}, :class => 'flickr_photo image', :border => 0)) %>
      </li>
    <% end %>
    </ul>
</div>